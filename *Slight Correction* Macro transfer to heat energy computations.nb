   3.678757308233251*^9, 3.678768357301464*^9}]
}, Open  ]],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"heatEnergyapprox3", "=", 
  RowBox[{"Assuming", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"\[Rho]", ">", "0"}], ",", 
      RowBox[{"\[Mu]", ">", "0"}]}], "}"}], ",", 
    RowBox[{"\[Pi]", "*", "h", "*", 
     RowBox[{"Integrate", "[", 
      RowBox[{
       RowBox[{"\[Mu]", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"v", "*", 
           RowBox[{"(", 
            RowBox[{"1", "-", 
             RowBox[{"Erf", "[", "\[Eta]", "]"}]}], ")"}]}], ")"}], "^", 
         "2"}]}], ",", 
       RowBox[{"{", 
        RowBox[{"\[Eta]", ",", "0", ",", 
         FractionBox[
          RowBox[{"k", " ", "\[Rho]"}], "\[Mu]"]}], "}"}]}], "]"}]}]}], 
   "]"}]}]], "Input",
